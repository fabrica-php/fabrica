{
	"name": "fabrica/fabrica",
	"description": "Library to create entity factories for testing",
	"type": "library",
	"keywords": [
		"testing",
		"factory"
	],
	"license": "MIT",
	"authors": [
		{
			"name": "Jonathan Taylor",
			"email": "jonny.taylor5@gmail.com"
		}
	],
	"scripts": {
		"test": "phpunit"
	},
	"require": {
		"php": "^7.0"
	},
	"require-dev": {
		"doctrine/orm": "^2.5",
		"phpunit/phpunit": "^7.5"
	},
	"suggest": {
		"fzaninotto/faker": "Use faker to auto-generate fake entity data"
	},
	"autoload": {
		"psr-4": {
			"Fabrica\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Fabrica\\Test\\": "test/"
		}
	},
	"config": {
		"sort-packages": true
	}
}
